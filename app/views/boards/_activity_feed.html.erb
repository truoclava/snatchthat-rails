<%@user.activity_feed.each do |activity|%>
    <br><br>
    <strong>
    <% if current_user.username == activity.user.username %>
    You
    <%else%>
   <%= link_to activity.user.username, activity.user %>
   <% end %>
  </strong>
   <br> 
    <% if activity.trackable_type == "Item" %>
      <em class = 'text-muted'><%=ActivityFeed.actionmessage(activity)%></em> <%=ActivityFeed.subject(activity)%> <em class = 'text-muted'> <%= ActivityFeed.preposition(activity) %> </em><%=ActivityFeed.closet(activity)%>
    <% else %>
      <em class = 'text-muted'> <%=ActivityFeed.actionmessage(activity)%></em> <%=ActivityFeed.subject(activity)%>
    <% end %>

<%end%>